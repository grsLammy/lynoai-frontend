# RainbowKit and Alchemy Environment Variables
# Copy this file to .env and replace the placeholder values with your actual keys

# WalletConnect Project ID (get one at https://cloud.walletconnect.com/)
VITE_WALLET_CONNECT_PROJECT_ID=YOUR_WALLET_CONNECT_PROJECT_ID

# Alchemy API Key (get one at https://www.alchemy.com/)
VITE_ALCHEMY_API_KEY=YOUR_ALCHEMY_API_KEY 

# LynoAI Contract Address
VITE_LYNOAI_CONTRACT_ADDRESS=YOUR_LYNOAI_CONTRACT_ADDRESS

# Current Phase
VITE_CURRENT_PHASE=YOUR_CURRENT_PHASE

# Current Price
VITE_CURRENT_PRICE=YOUR_CURRENT_PRICE

# USDT Contract Address
VITE_USDT_CONTRACT_ADDRESS=YOUR_USDT_CONTRACT_ADDRESS

# USDC Contract Address
VITE_USDC_CONTRACT_ADDRESS=YOUR_USDC_CONTRACT_ADDRESS